{
    "name": "webjam",
    "description": "A straightforward web framework build for common tasks.",
    "version": "1995.3.7",
    "private": false,
    "homepage": "https://github.com/Leask/webjam",
    "main": "index.mjs",
    "type": "module",
    "engines": {
        "node": ">=18.x"
    },
    "scripts": {
        "start": "node --no-warnings main.mjs",
        "debug": "node --no-warnings --inspect --trace-warnings debug.mjs",
        "test": "node --no-warnings --trace-warnings test.mjs",
        "updep": "npx npm-check-updates -u && npm install",
        "gitsync": "( git commit -am \"Released @ `date`\" || true ) && git pull && git push",
        "build": "npm run updep && ( git commit -am 'update dependencies' || true )",
        "pub": "npm run build && npm run gitsync",
        "beta": "npm publish --tag beta"
    },
    "author": "Leask Wong <i@leaskh.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Leask/webjam"
    },
    "dependencies": {
        "@koa/multer": "^4.0.0",
        "@koa/router": "^15.3.0",
        "acme-client": "^5.4.0",
        "fast-geoip": "^1.1.88",
        "http-status": "^2.1.0",
        "koa": "^3.1.1",
        "koa-bodyparser": "^4.4.1",
        "koa-json": "^2.0.2",
        "koa-logger": "^4.0.0",
        "koa-session": "^7.0.2",
        "koa-static": "^5.0.0",
        "koa-useragent": "^4.1.0",
        "lorem-ipsum": "^2.0.8",
        "multer": "^2.0.2",
        "utilitas": "^2001.1.128"
    },
    "devDependencies": {
        "@google-cloud/storage": "^7.19.0",
        "@sentry/node": "^10.39.0",
        "@sentry/profiling-node": "^10.39.0",
        "@sentry/utils": "^8.55.0",
        "domain": "^0.0.1",
        "form-data": "^4.0.5",
        "ioredis": "^5.9.3",
        "koa-passport": "^6.0.0",
        "mailgun.js": "^12.7.0",
        "mysql2": "^3.17.3",
        "node-mailjet": "^6.0.11",
        "openai": "^6.22.0",
        "passport-twitter": "^1.0.4",
        "pg": "^8.18.0",
        "pgvector": "^0.2.1",
        "ping": "^1.0.0",
        "telegraf": "^4.16.3",
        "telesignsdk": "^5.0.0",
        "twilio": "^5.12.2"
    }
}
