{
    "name": "webjam",
    "description": "A straightforward web framework build for common tasks.",
    "version": "1994.1.27",
    "private": false,
    "homepage": "https://github.com/Leask/webjam",
    "main": "index.mjs",
    "type": "module",
    "engines": {
        "node": ">=18.x"
    },
    "scripts": {
        "start": "node --no-warnings main.mjs",
        "debug": "node --no-warnings --inspect --trace-warnings debug.mjs",
        "test": "node --no-warnings --trace-warnings test.mjs",
        "updep": "npx npm-check-updates -u && npm install",
        "gitsync": "( git commit -am \"Released @ `date`\" || true ) && git pull && git push",
        "build": "npm run updep && ( git commit -am 'update dependencies' || true )",
        "pub": "npm run build && npm run gitsync",
        "beta": "npm publish --tag beta"
    },
    "author": "Leask Wong <i@leaskh.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Leask/webjam"
    },
    "dependencies": {
        "@koa/multer": "^3.1.0",
        "@koa/router": "^13.1.0",
        "acme-client": "^5.4.0",
        "fast-geoip": "^1.1.88",
        "http-status": "^2.1.0",
        "koa": "^2.16.0",
        "koa-bodyparser": "^4.4.1",
        "koa-json": "^2.0.2",
        "koa-logger": "^3.2.1",
        "koa-session": "^7.0.2",
        "koa-static": "^5.0.0",
        "koa-useragent": "^4.1.0",
        "lorem-ipsum": "^2.0.8",
        "multer": "^1.4.4",
        "utilitas": "^1999.1.46"
    },
    "devDependencies": {
        "@google-cloud/storage": "^7.15.2",
        "@sentry/node": "^9.9.0",
        "@sentry/profiling-node": "^9.9.0",
        "@sentry/utils": "^8.55.0",
        "domain": "^0.0.1",
        "form-data": "^4.0.2",
        "ioredis": "^5.6.0",
        "koa-passport": "^6.0.0",
        "mailgun.js": "^12.0.1",
        "mysql2": "^3.14.0",
        "node-mailjet": "^6.0.8",
        "openai": "^4.89.1",
        "passport-twitter": "^1.0.4",
        "pg": "^8.14.1",
        "pgvector": "^0.2.0",
        "ping": "^0.4.4",
        "telegraf": "^4.16.3",
        "telesignsdk": "^3.0.2",
        "twilio": "^5.5.1"
    }
}
